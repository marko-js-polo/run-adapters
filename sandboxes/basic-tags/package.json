{
  "name": "basic-tags",
  "description": "The default Marko starter app with TypeScript",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "marko": "^6.0.0"
  },
  "devDependencies": {
    "@antfu/ni": "^24.3.0",
    "@marko-polo/run-adapter-single-file": "workspace:*",
    "@marko/run": "^0.6.4",
    "@marko/run-adapter-static": "^1.0.2",
    "vite": "^6.3.0"
  },
  "private": true,
  "scripts": {
    "dev": "marko-run",
    "build": "nr build:static; nr build:single-file",
    "build:static": "marko-run build --config vite.config.static.ts",
    "build:single-file": "marko-run build --config vite.config.single-file.ts",
    "preview": "marko-run preview",
    "start": "node --enable-source-maps ./dist/index.mjs"
  }
}